  "code": "//Hello AI world!\nvar videos = [];\n\nfunction setup() {\n    var add = button = createButton(\"增加\");\n    add.position(240, 20);\n    add.mousePressed(addEvent);\n    var index = 0;\n    function addEvent() {\n        index++;\n        createVideoInput(index, 10, 50 + 150 * (index - 1), \"素材\");\n    }\n    var button2 = createButton(\"合成\");\n    button2.position(300, 20);\n    button2.mousePressed(buttonEvent2);\n    function buttonEvent2() {\n        //alert(JSON.stringify(videos,null,2))\n        var videosNew = Array.from(videos, v => {\n            if (v && v.url) {\n                return {\n                    url: v.url,\n                    text: v.text,\n                    type: v.type,\n                    textImage: Lab.base.createTextImage(v.text, 120, \"red\")\n                }\n            }\n        }).filter(f => f);\n        //alert(JSON.stringify(videos,null,2))\n        Lab.video.createShortVideoFromLocal(videosNew).then(()=>alert('完成'));\n    }\n\n};\n\nfunction createVideoInput(index, x, y, displayText) {\n    videos[index] = {\n        url: null,\n        text: null,\n        type: null\n    };\n    var button = createButton(displayText);\n    button.position(x, y);\n    button.mousePressed(buttonEvent);\n\n    function buttonEvent() {\n        var labVideo = Lab.video.createShortVideoInput();\n        if (labVideo) {\n            videos[index].url = labVideo.url;\n            videos[index].type = labVideo.type;\n            if (labVideo.type == \"video\") {\n                var vid = createVideo(labVideo.url, vidLoad);\n                vid.size(100, 100);\n                vid.position(x + 100, y);\n\n                function vidLoad() {\n                    vid.loop();\n                    //vid.pause();\n                    vid.volume(0.8);\n                }\n            };\n            if (labVideo.type == \"img\") {\n                var vid = createVideo(labVideo.url);\n                vid.size(100, 100);\n                vid.position(x + 100, y);\n                vid.attribute('poster', labVideo.url);\n            };\n            if (labVideo.type == \"audio\") {\n\n                var ele = createAudio(labVideo.url);\n                //ele.autoplay(false);\n                var abtn = createButton(\"音频-点击播放\");\n                abtn.position(x + 100, y + 50);\n                abtn.mousePressed(buttonEvent3);\n                function buttonEvent3() {\n                    //\n                    ele.play(true);\n                }\n            };\n\n            if (labVideo.type != \"audio\") {\n                let inp = createInput('');\n                inp.input(myInputEvent);\n                inp.position(x, y + 120);\n\n                function myInputEvent() {\n                    videos[index].text = (this.value()).trim();\n                }\n            }\n\n        }\n    }\n}",
