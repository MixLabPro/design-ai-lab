  "code": "//Hello AI world!\nvar layout = new Canvas(300, 300, false, true);\n// 双击移动到最顶部\nlayout.dblclick = opt => {\n    layout.moveToTop(opt.target);\n};\n\n\nvar yolo, u2net;\n\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    Lab.base.createInput('img', 'text', doImage);\n    Lab.base.add(layout.getElement());\n    yolo = new Lab.ai.Yolov5();\n    yolo.load();\n    u2net = Lab.ai.u2net;\n};\n\nasync function doImage(url) {\n    layout.reset();\n    let im = await Lab.base.createImage(url);\n    layout.addImg(im, {\n        lockMovementX: true,\n        lockRotation: true,\n        lockScalingX: true,\n        lockScalingY: true,\n    }, false, false);\n    layout.zoomToFitCanvas();\n    await doResearch(im);\n};\n\nasync function doResearch(img) {\n    // 文本检测\n    let texts = await Lab.ai.getText(img);\n    console.log(texts)\n    texts.forEach(t => {\n        t = Object.assign(t, {\n            fill: 'rgba(255,0,0,0.4)',\n            stroke: 'red',\n            left: t.x,\n            top: t.y\n        });\n        addRect(t);\n    });\n\n    // 多目标检测\n    let boxes = await yolo.detectAndBox(img);\n    console.log(boxes)\n    boxes.forEach(t => {\n        t = Object.assign(t, {\n            fill: 'rgba(0,255,0,0.4)',\n            stroke: 'rgba(0,255,0,1)'\n        });\n        addRect(t);\n    });\n    let seg = await u2net.drawSegment(img);\n    // console.log(seg)\n    // 外围父级轮廓\n    let rects = Lab.ai.shape.getBoundingRect(seg);\n    if (rects.length > 0) {\n        console.log(rects)\n        var t = Object.assign(rects[0], {\n            fill: 'rgba(0,0,255,0.4)',\n            stroke: 'rgba(0,0,255,1)',\n            left: rects[0].x,\n            top: rects[0].y\n        });\n        addRect(t);\n    }\n\n}\n\nfunction addRect(style) {\n    layout.addRect(Object.assign(style, {\n        lockMovementX: false,\n        lockRotation: false,\n        lockScalingX: false,\n        lockScalingY: false,\n    }), true, true)\n}",
