  "code": "function gui() {\n    //隐藏p5的画布\n    Lab.base.p5Show(false);\n    // 标题\n    let title = Lab.base.createTextCanvas('内联图片');\n    title.style.height = '24px';\n\n    // 上传控件\n    Lab.base.createInput('img', \"图片\", async (url) => {\n        let width = ~~size.querySelector('input').value;\n        let img = await Lab.base.createImage(url);\n        let height = parseInt(img.naturalHeight * width / img.naturalWidth);\n        canvas.width = width;\n        canvas.height = height;\n        ctx.drawImage(img, 0, 0, width, height);\n        let base64 = canvas.toDataURL();\n        let html = `<img src='${base64}' style='width:${width}px;height:${height}px'/>`;\n        // console.log(width,size)\n        Lab.base.clipboard.write(html, 'text');\n    }, true);\n\n    //创建输入\n    let size = Lab.base.createInput('text', '宽度');\n\n    // 用于压缩图片\n    let canvas = Lab.base.createCanvas(100, 100, 'canvas', 'myCanvas', true);\n    canvas.style.margin = '24px';\n    let ctx = canvas.getContext('2d');\n    ctx.fillRect(0, 0, 100, 100);\n\n}",
