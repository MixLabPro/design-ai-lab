  "code": "//Hello AI world!\nvar model, im, base64, layout = new Canvas(300, 300);\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    model = Lab.ai.u2net;\n    Lab.base.add(layout.getElement());\n\n    let input = Lab.base.createInput('img', \"图片\", async url => {\n        let img = await Lab.base.createImage(url);\n\n        let fileInfo = await Lab.video.getMediaDurationAndType(url);\n        console.log(fileInfo);\n        if (fileInfo.type === 'gif' || fileInfo.type === 'video') {\n\n            let frames = await Lab.video.videoExtract(url, fileInfo.width + ':' + fileInfo.height, null, fadeIn = 0, fadeOut = 0, fps = fileInfo.frame_rate)\n            frames = frames.images;\n\n            let spirit = document.createElement('canvas');\n            spirit.width = frames.length * fileInfo.width;\n            spirit.height = fileInfo.height;\n            let ctx = spirit.getContext('2d');\n            let index = 0;\n            //let g = new Lab.base.GIF();\n            for (let f of frames) {\n                let seg = await model.segment(await Lab.base.createImage(f));\n                ctx.drawImage(seg, 0, 0, seg.width, seg.height, index * seg.width, 0, seg.width, seg.height);\n                // g.add(seg, fileInfo.frame_rate);\n                index++;\n                console.log(index / frames.length, frames.length);\n            };\n            // base64 = await g.render();\n            base64 = spirit.toDataURL();\n            let gImg = await Lab.base.createImage(base64);\n            im.innerHTML = \"\";\n            im.appendChild(gImg);\n            // im.appendChild(spirit);\n\n            // 预览 \n            await layout.addSprite(base64, {\n                width: fileInfo.width,\n                height: fileInfo.height,\n                frameTime: 1000 / fileInfo.frame_rate\n            });\n            layout.zoomToFitCanvas();\n\n        } else if (fileInfo.type === 'img') {\n            // images.push(img);\n            model.segment(img).then(c => {\n                c.style = `width: auto;\n                margin-top: 32px;\n                max-height: 300px;\n                max-width: 300px;`;\n                im.innerHTML = \"\";\n                base64 = c.src;\n                im.appendChild(c);\n            });\n        };\n\n    }, true, false);\n\n    im = Lab.base.createGroup();\n\n    let g = Lab.base.createGroup(input, im);\n    // g.layout(1);\n    Lab.base.add(g);\n\n    let download = Lab.base.createIcon('download', () => {\n        Lab.base.clipboard.write(base64, 'base64');\n        var imd = Lab.base.clipboard.read('img');\n        Lab.base.clipboard.clear();\n        Lab.base.saveNativeImageDialog(imd);\n    }, false);\n\n    let copy = Lab.base.createIcon('copy', () => {\n        Lab.base.clipboard.write(base64, 'base64');\n    }, false);\n\n    Lab.base.add(Lab.base.createGroup(download, copy));\n\n}",
