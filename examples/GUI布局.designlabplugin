  "code": "\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    Lab.base.layout(0);\n  \n    Array.from([\n        // 默认，水平布局，自动换行\n        'default',\n        // 垂直左对齐\n        'lv',\n        // 垂直居中\n        'cv',\n        // 垂直右对齐\n        'rv',\n        // 网格3:7\n        'g37'\n    ], t => layout(t));\n    \n    // 九宫格\n    g9();\n};\n\n\nfunction layout(text = 'default') {\n    // 新建画布\n    let canvas = new Canvas(windowWidth / 3, windowWidth / 3);\n    // 文本\n    let info = Lab.base.createBaseText(text);\n\n    // 画布的HTMLElement\n    let g1 = Lab.base.createGroup(\n        canvas.getElement(),\n        info,\n        Lab.base.createButton('按钮', () => { }, false),\n        Lab.base.createIcon('refresh', () => { }, false)\n    );\n\n    // 开启布局调试\n    g1.layout(text, true);\n    g1.style.marginBottom = '24px';\n    // 添加\n    Lab.base.add(g1);\n}\n\nfunction g9() {\n    // 画布的HTMLElement\n\n    let g1 = Lab.base.createGroup(\n        ...Array.from(\n            new Array(9), a => (new Canvas(windowWidth*0.29, windowWidth*0.29)).getElement()\n        )\n    );\n    // console.log(g1)\n    // 开启布局调试\n    g1.layout('g9');\n    g1.style.marginBottom = '24px';\n    // 添加\n    Lab.base.add(g1);\n\n}",
