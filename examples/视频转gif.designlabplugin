  "code": "\nvar width = 220, fps = 10, startTime = 0, loop = 5;\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    let p = Lab.base.createBaseText('---');\n\n    let btn = Lab.base.createButton(\"视频转动图\", async () => {\n        let v = await Lab.base.createShortVideoInput();\n        if (v[0].type === 'video') {\n            p.innerText = '正在ing';\n            let output = await Lab.video.video2gif(v[0].url, width, fps, startTime, loop);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n    let btn2 = Lab.base.createButton(\"视频转mp4\", async () => {\n        let v = await Lab.base.createShortVideoInput();\n        if (v[0].type === 'video') {\n            p.innerText = '正在ing';\n            let output = await Lab.video.video2mp4(v[0].url);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n\n    let btn3 = Lab.base.createButton(\"动图转视频\", async () => {\n        let v = await Lab.base.createShortVideoInput();\n        console.log(v)\n        if (v[0].type === 'gif') {\n            p.innerText = '正在ing';\n            let output = await Lab.video.gif2video(v[0].url);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n    // let p=Lab.base.createBaseText('---');\n\n    let g = Lab.base.createGroup(p, btn, btn2, btn3);\n    g.layout(2);\n    Lab.base.add(g);\n\n    Lab.base.createInput('text', '宽度', text => {\n        if (text) width = parseInt(text);\n    });\n    Lab.base.createInput('text', '帧速率', text => {\n        if (text) fps = parseInt(text);\n    });\n    Lab.base.createInput('text', '开始时间', text => {\n        if (text) startTime = parseInt(text);\n    });\n    Lab.base.createInput('text', '终止时间', text => {\n        if (text) loop = parseInt(text);\n    });\n\n}",
