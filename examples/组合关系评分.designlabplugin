    "course": "\n\n- 自动监听剪切板，收集图片\n- 轮廓比较，结果越低，匹配越好\n\n"
  },
  "code": "// 所有图片\nvar imagesDict = {};\n\n// 存储图片特征\nvar features = {\n    1: null,\n    2: null\n};\n\nfunction gui() {\n    //隐藏p5的画布\n    // Lab.base.p5Show(false);\n    Lab.base.layout('oneColumn');\n    // 清空图片\n    Lab.base.createIcon('clear', () => {\n        // 清空缓存\n        Lab.base.clearAllClipboardStore('img');\n        for (let id in imagesDict) {\n            imagesDict[id].remove();\n        };\n    });\n    // 下一张组合\n    //创建图标\n    Lab.base.createIcon('refresh', () => {\n        draw();\n        setTimeout(()=>{\n            // 结果越低，匹配越好\n            let c=compare(features[1].cnt,features[2].cnt);\n            console.log(c);\n            Lab.base.add(features[3]);\n        },1000);\n    });\n    \n    // 尝试下打标，把构图比较好的轮廓作为参考，比较轮廓\n    function score(){}\n\n    // 监听剪切板\n    Lab.base.clipboardListener('img', async (nativeImage) => {\n        let url = nativeImage.toDataURL();\n        // console.log(nativeImage.toDataURL())\n\n        //创建图片\n        await addImg(url);\n    });\n    // 获取缓存\n    Lab.base.getAllClipboardStore('img').then(async json => {\n        for (let id in json) {\n            let base64 = json[id];\n            await addImg(base64);\n        }\n    });\n\n    // 图片\n    async function addImg(url) {\n        let id = Lab.base.md5(url);\n        if (imagesDict[id]) return;\n        let img = await Lab.base.createImage(url);\n        img.style.width = \"88px\";\n        img.style.margin = \"8px\";\n        img.id = id;\n        imagesDict[id] = img;\n        let cannyImg = await imgProcess(img);\n        let {thresholdImg,res} = await threshold(img,100,100);\n        // area(img);\n        Lab.base.add(Lab.base.createGroup(img, cannyImg, thresholdImg));\n    }\n}\n\n// 边缘检测\nasync function imgProcess(img) {\n    //创建画布\n    let canvas = Lab.base.createCanvas(img.naturalWidth, img.naturalHeight, 'myCanvas', '', false);\n    let src = cv.imread(img);\n\n    // 缩放为100x100方形\n    let rdst = new cv.Mat();\n    let dsize = new cv.Size(100, 100);\n    // You can try more different parameters\n    cv.resize(src, rdst, dsize, 0, 0, cv.INTER_AREA);\n\n    let dst = new cv.Mat();\n    cv.cvtColor(rdst, rdst, cv.COLOR_RGB2GRAY, 0);\n    // You can try more different parameters\n    cv.Canny(rdst, dst, 50, 100, 3, false);\n\n    cv.imshow(canvas, dst);\n\n    let url = canvas.toDataURL();\n    let cannyImg = await Lab.base.createImage(url);\n    //img.insertAdjacentElement('afterend',cannyImg);\n    // console.log(cannyImg);\n    src.delete();\n    rdst.delete();\n    dst.delete();\n    return cannyImg;\n}\n\n// 阈值分割\nasync function threshold(img,w,h) {\n    //创建画布\n    let canvas = Lab.base.createCanvas(img.naturalWidth, img.naturalHeight, 'myCanvas', '', false);\n    let src = cv.imread(img);\n    cv.cvtColor(src, src, cv.COLOR_RGBA2GRAY, 0);\n\n    // 缩放为100x100方形\n    let rdst = new cv.Mat();\n    let dsize = new cv.Size(w,h);\n    // You can try more different parameters\n    cv.resize(src, rdst, dsize, 0, 0, cv.INTER_AREA);\n\n    let dst = new cv.Mat();\n    // You can try more different parameters\n    cv.threshold(rdst, dst, 10, 100, cv.THRESH_BINARY);\n    cv.imshow(canvas, dst);\n\n    let url = canvas.toDataURL();\n    let thresholdImg = await Lab.base.createImage(url);\n\n    // console.log(area(dst))\n    let res=area(dst);\n    // res=res/(w*h);\n\n    rdst.delete();\n    src.delete();\n    //dst.delete();\n    return {thresholdImg,res:res};\n}\n\n// 轮廓\nfunction area(src) {\n    let values = [];\n    let contours = new cv.MatVector();\n    let hierarchy = new cv.Mat();\n    cv.findContours(src, contours, hierarchy, cv.RETR_CCOMP, cv.CHAIN_APPROX_SIMPLE);\n\n    for (let i = 0; i < contours.size(); i++) {\n        let cnt = contours.get(i);\n        let area = cv.contourArea(cnt, false);\n        values.push({\n            area:area,\n            cnt:cnt\n        });\n    };\n    values=values.sort((b,a)=>a.area-b.area)\n    src.delete();\n    contours.delete();\n    hierarchy.delete();\n    return values[0]\n}\n\n//  比较两个轮廓\nfunction compare(cnt1, cnt2){\n    let result = cv.matchShapes(cnt1, cnt2, 1, 0);\n    return result;\n}\n\nfunction setup() {\n    // frameRate(3);\n    noLoop();\n    createCanvas(windowWidth, 300);\n    noFill();\n}\n\nasync function draw() {\n    background('rgba(0,0,0,0)');\n    clear();\n    stroke('red');\n    let r1 = [width * 0.15, height * 0.2, width * 0.6, height * 0.7],\n        r2 = [width * 0.6, height * 0.5, width * 0.3, height * 0.3];\n    rect(...r1);\n    rect(...r2);\n    if (Object.keys(imagesDict).length > 0) {\n        let img1 = randomOne();\n        let w = r1[2];\n        let h = img1.height * w / img1.width;\n        let y = 0;\n        y = h < r1[3] ? height - h : r1[1];\n\n        image(img1, r1[0], y, w, h);\n        // 特征提取\n        let {thresholdImg,res} = await threshold(img1.elt,w,h);\n        features[1] = res;\n    };\n    if (Object.keys(imagesDict).length > 0) {\n        let img2 = randomOne();\n        let w = r2[2];\n        let h = img2.height * w / img2.width;\n        let y = 0;\n        y = h < r2[3] ? height - h : r2[1];\n\n        image(img2, r2[0], y, w, h);\n\n        // 特征提取\n        var {thresholdImg,res} = await threshold(img2.elt,w,h);\n        features[2] = res;\n\n        // 合图后的特征\n        var {thresholdImg,res} = await threshold(canvas,width,height);\n        features[3] = thresholdImg;\n    };\n}\n\nfunction randomOne() {\n    let imgs = [];\n    for (let id in imagesDict) {\n        let img = imagesDict[id];\n        imgs.push(img);\n    };\n    let img = imgs[parseInt(Math.random() * imgs.length)];\n    //console.log(imgs)\n    return Lab.ai.p5Image(img)\n}",
