{
    "name": "design-ai-lab",
    "version": "0.1.0",
    "description": "智能设计实验工具 \nArtificial Intelligence for Design",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "NODE_ENV=development electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder --macos --linux --windows"
    },
    "homepage": "https://github.com/shadowcz007/design-ai-lab/",
    "repository": "https://github.com/shadowcz007/design-ai-lab.git",
    "bugs": "https://github.com/shadowcz007/design-ai-lab/issues",
    "keywords": [
        "Javascript",
        "Tensorflow.js",
        "p5.js",
        "Design.ai Lab",
        "Code Examples"
    ],
    "author": "shadow",
    "license": "MIT",
    "devDependencies": {
        "electron": "^11.2.1",
        "electron-builder": "^22.9.1"
    },
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.0.20",
        "@fortawesome/fontawesome-free": "^5.15.2",
        "@hapi/hapi": "^20.1.0",
        "@tensorflow-models/body-pix": "^2.0.5",
        "@tensorflow-models/deeplab": "^0.2.1",
        "@tensorflow-models/knn-classifier": "^1.2.2",
        "@tensorflow-models/mobilenet": "^2.0.4",
        "@tensorflow/tfjs": "^3.0.0",
        "about-window": "^1.13.4",
        "blockly": "^4.20201217.0",
        "color": "^3.1.3",
        "colorthief": "^2.3.2",
        "electron-json-storage": "^4.3.0",
        "escodegen": "^2.0.0",
        "esprima": "^4.0.1",
        "estraverse": "^5.2.0",
        "fluent-ffmpeg": "^2.1.2",
        "jimp": "^0.16.1",
        "lowdb": "^1.0.0",
        "marked": "^1.2.8",
        "md5": "^2.3.0",
        "mermaid": "^8.9.0",
        "monaco-editor": "^0.21.3",
        "muuri": "^0.9.3",
        "ngraph.path": "^1.3.1",
        "object-hash": "^2.1.1",
        "opencvjs-dist": "^4.4.0",
        "p5": "^1.2.0",
        "p5.createloop": "^0.2.8",
        "pdfjs-dist": "^2.6.347",
        "resizer-cl": "^2.4.0",
        "timeago.js": "^4.0.2"
    },
    "build": {
        "appId": "mixlab.designailab",
        "mac": {
            "category": "shadow.app.design.ai.lab"
        },
        "fileAssociations": [
            {
                "ext": "designlabplugin",
                "name": "DesignLab File",
                "role": "Editor",
                "perMachine": true
            }
        ]
    }
}
